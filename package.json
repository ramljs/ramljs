{
  "name": "ramljs",
  "description": "RAML server framework for Javascript",
  "version": "0.0.1",
  "author": "Panates",
  "contributors": [
    "Eray Hanoglu <e.hanoglu@panates.com>",
    "Arhun Hınçalan <ahincalan@hotmail.com>"
  ],
  "browser": "./lib/browser.js",
  "main": "./lib/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ramljs/ramljs.git"
  },
  "keywords": [
    "javascript",
    "raml",
    "framework",
    "api"
  ],
  "dependencies": {
    "ajv": "^6.10.2",
    "fast-json-stringify": "^1.15.3",
    "glob": "^7.1.4",
    "raml-1-parser": "^1.1.55",
    "router": "^1.3.3",
    "string-replace-async": "^1.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/glob": "^7.1.1",
    "@types/ajv": "^1.0.0",
    "express": "^4.17.1",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">= 8.0"
  },
  "files": [
    "LICENSE",
    "README.md",
    "lib/"
  ],
  "nyc": {
    "temp-dir": "./coverage/.nyc_output",
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*.ts",
      "src/**/*.js"
    ],
    "exclude": [
      "**/*.d.ts"
    ]
  },
  "scripts": {
    "test": "mocha -r ./test/support/env -r ts-node/register --reporter spec --bail --check-leaks ./test/*.ts --timeout 20000",
    "cover": "nyc --reporter html --reporter text -r lcov npm run test",
    "travis-cover": "nyc --reporter lcovonly -r lcov npm run test",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "tsc"
  }
}
