#%RAML 1.0
title: Starwars example API
description: Starwars example API
documentation:
 - title: Home
   content: ---
version: 1
protocols: [https]
baseUri: https://api.starwars.com

types:
  Person:
    type: object
    discriminator: kind # refers to the `kind` property of object `Person`
    properties:
      kind: string # contains name of the kind of a `Person` instance
      name: string
  Employee: # kind can equal `Employee`; default value for `discriminatorValue`
    type: Person
    properties:
      employeeId: integer
  User: # kind can equal `User`; default value for `discriminatorValue`
    type: Person
    properties:
      userId: integer
  type1:
    type: [User, Employee]
